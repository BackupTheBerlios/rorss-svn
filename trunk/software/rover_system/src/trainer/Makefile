integrate:
	echo -n "  $(LIBS) " >> ../../project_compile_space/_LIBS
	echo -n " `pwd` " >> ../../project_compile_space/_INCLUDES
	for each in $(OBJECTS) $(OBJMOC) ; do echo -n " `pwd`/$$each" >> ../../project_compile_space/_OBJECTS; done



staticlib: $(TARGETA)

$(TARGETA):  $(UICDECLS) $(OBJECTS) $(OBJMOC) $(OBJCOMP)
	-$(DEL_FILE) $(TARGETA) 
	$(AR) $(TARGETA) $(OBJECTS) $(OBJMOC)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

