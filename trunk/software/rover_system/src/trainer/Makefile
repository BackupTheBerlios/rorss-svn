####### Compile

.obj/main.o: src/main.cpp .ui/trainer.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o src/main.cpp

.obj/train.o: src/train.cpp src/train.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/train.o src/train.cpp

.ui/trainer.h: src/trainer.ui src/train.h
	$(UIC) src/trainer.ui -o .ui/trainer.h

.ui/trainer.cpp: .ui/trainer.h src/trainer.ui src/train.h
	$(UIC) src/trainer.ui -i trainer.h -o .ui/trainer.cpp

.obj/trainer.o: .ui/trainer.cpp src/trainer.ui.h \
		.ui/trainer.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/trainer.o .ui/trainer.cpp

.obj/moc_trainer.o: .moc/moc_trainer.cpp .ui/trainer.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_trainer.o .moc/moc_trainer.cpp

.moc/moc_trainer.cpp: $(MOC) .ui/trainer.h
	$(MOC) .ui/trainer.h -o .moc/moc_trainer.cpp

.obj/qmake_image_collection.o: .ui/qmake_image_collection.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qmake_image_collection.o .ui/qmake_image_collection.cpp

.ui/qmake_image_collection.cpp: images/filenew.png \
		images/fileopen.png \
		images/filesave.png \
		images/print.png \
		images/undo.png \
		images/redo.png \
		images/editcut.png \
		images/editcopy.png \
		images/editpaste.png \
		images/searchfind.png \
		src/glove.png \
		src/glove2.png
	$(UIC)  -embed GloveTrainer images/filenew.png images/fileopen.png images/filesave.png images/print.png images/undo.png images/redo.png images/editcut.png images/editcopy.png images/editpaste.png images/searchfind.png src/glove.png src/glove2.png -o .ui/qmake_image_collection.cpp

