dist: 
	@mkdir -p .obj/GloveTrainer && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/GloveTrainer/ && $(COPY_FILE) --parents images/filenew.png images/fileopen.png images/filesave.png images/print.png images/undo.png images/redo.png images/editcut.png images/editcopy.png images/editpaste.png images/searchfind.png src/glove.png src/glove2.png .obj/GloveTrainer/ && $(COPY_FILE) --parents src/trainer.ui.h .obj/GloveTrainer/ && ( cd `dirname .obj/GloveTrainer` && $(TAR) GloveTrainer.tar GloveTrainer && $(GZIP) GloveTrainer.tar ) && $(MOVE) `dirname .obj/GloveTrainer`/GloveTrainer.tar.gz . && $(DEL_FILE) -r .obj/GloveTrainer

